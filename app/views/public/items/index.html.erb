
<%= render 'public/genres/list', genres: @genres %>

<h2><%= @list_title %>一覧</h2>
<% @items.each do |item| %>
<div>
  <%= link_to item_path(item) do %>
  <%= attachment_image_tag item, :image, :fill, 60, 60, format: 'jpeg',
  fallback: "no_image.jpg" %><br>
  <%= item.name %><br>
  <% end %>
  \<%= item.add_tax_price %>円
</div>
<% end %>