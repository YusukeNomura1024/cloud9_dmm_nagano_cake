<h1>ジャンル編集</h1>
<div class="edit-genre">
<%= form_with model: @genre,url: admin_genre_path, local: true do |f| %>
  <div class="form-group">
    <label for="genre_name">ジャンル名</label>
    <%= f.text_field :name, class: "from-control genre_name", id: "jenre_name" %>
  </div>
  <%= f.submit "変更を保存" %>
<% end %>
</div>
